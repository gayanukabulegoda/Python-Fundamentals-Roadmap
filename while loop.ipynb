   "source": [
    "# print all square numbers less than a given number\n",
    "\n",
    "num = int(input(\"Enter a number: \"))\n",
    "square_root_value_list = []\n",
    "\n",
    "i = 1\n",
    "while i**2 <= num:\n",
    "    square_root_value_list.append(i**2)\n",
    "    i += 1\n",
    "\n",
    "print(square_root_value_list)"
   ]
  }
